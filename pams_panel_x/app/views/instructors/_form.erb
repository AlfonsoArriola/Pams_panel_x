<%= form_for  @instructor do |f| %>
		  <div>
		 	 <%= f.label :first_name %>
		 	 <%= f.text_field :first_name %>
		  </div>

		  <div>
		 	 <%= f.label :last_name %>
		 	 <%= f.text_field :last_name %>
		  </div>

		  <div>
		 	 <%= f.label :highest_completed_education%>
		 	 <%= f.text_field :highest_completed_education %>
		 </div>

		 <div>
		 	 <%= f.label :age %>
		 	 <%= f.text_field :age %>
		 </div>

		 <div>
		 	 <%= f.label :salary %>
		 	 <%= f.text_field :salary %>
		 </div>


		 <div>
		  	<%= f.submit %>
		 </div>

<% end %>