<%= form_for  @student do |f| %>
		  <div>
		 	 <%= f.label :first_name %>
		 	 <%= f.text_field :first_name %>
		  </div>

		  <div>
		 	 <%= f.label :last_name %>
		 	 <%= f.text_field :last_name %>
		  </div>

		  <div>
		 	 <%= f.label :highest_education_completed %>
		 	 <%= f.text_field :highest_education_completed %>
		 </div>

		 <div>
		 	 <%= f.label :age %>
		 	 <%= f.text_field :age %>
		 </div>

		


		 <div>
		  	<%= f.submit %>
		 </div>

<% end %>