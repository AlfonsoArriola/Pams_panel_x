<%= form_for  @cohort do |f| %>
		  <div>
		 	 <%= f.label :name %>
		 	 <%= f.text_field :name %>
		  </div>

		  <div>
		 	 <%= f.label :start_date %>
		 	 <%= f.text_field :start_date %>
		  </div>

		  <div>
		 	 <%= f.label :end_date %>
		 	 <%= f.text_field :end_date %>
		  </div>

		  <div>
		 	 <%= f.label :instructor_id %>
		 	 <%= f.text_field :instructor_id %>
		  </div>

		  <div>
		  	 <%= f.submit %>
		  </div>

<% end %>